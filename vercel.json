{
  "version": 2,
  "builds": [
    { "src": "studybud/api/*.py", "use": "@vercel/python" },
    { "src": "studybud/frontend/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/" },
    { "src": "/(.*)", "dest": "/" , "headers": { "Cache-Control": "public, max-age=0, must-revalidate" } }
  ]
}
